{
  "inlayHint.enable": false,
  "suggest.noselect": true,
  "diagnostic.messageTarget": "float",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": " <- ",

  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.checkUpdate": false,

  "Lua.diagnostics.globals": [ "api", "jit", "vim" ],
  "Lua.diagnostics.disable": [ "inject-field" ],
  "Lua.runtime.version": "LuaJIT",
  "Lua.workspace.library": [
    "/usr/local/share/nvim/runtime/lua",
    "/usr/local/share/nvim/runtime/lua/vim",
    "/usr/local/share/nvim/runtime/lua/vim/lsp",
    "/usr/local/share/nvim/runtime/lua/vim/treesitter"
  ],

  "java.compile.nullAnalysis.mode": "automatic",

  "clangd.checkUpdates": false,
  "clangd.fallbackFlags": [
    "-I/usr/lib/jvm/default/include",
    "-I/usr/lib/jvm/default/include/linux"
  ],

  "rust-analyzer.updates.checkOnStartup": false,
  "rust-analyzer.updates.channel": "stable",

  "snippets.ultisnips.pythonPrompt": false
}
